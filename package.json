{
  "name": "search-medium-ai-blog",
  "displayName": "Search Medium AI Blogs",
  "description": "Search Medium AI blogs",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/souyang/search-medium-ai-blog"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "search-medium-ai-blog.searchMediumAiBlog",
        "title": "Search Medium AI Blogs"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out --skipLibCheck",
    "watch-tests": "tsc -p . -w --outDir out  --skipLibCheck",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "prepare": "husky install",
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": "tsc-files --noEmit"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.3",
    "@types/vscode": "^1.84.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1",
    "axios-cache-interceptor": "^1",
    "fast-xml-parser": "^4.3.2",
    "regenerator-runtime": "^0.14.0",
    "xml2js": "^0.6.2"
  },
  "license": "MIT"
}
